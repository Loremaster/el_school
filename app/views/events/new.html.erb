<legend>Создание мероприятия <%= @class_code %> класса</legend>

<%= form_for @event, :html => {:class => "form-horizontal"} do |f| %>
  <%= field_set_tag do %>
    <%= render :partial => 'event_fields', :locals => { :f => f } %>

    <%= f.submit "Создать", :class => "btn btn-large btn-success" %>
  <% end %>
<% end %>